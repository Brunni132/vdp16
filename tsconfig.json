  "compilerOptions": {
    "outDir": "./temp/",
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "downlevelIteration": true,
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "allowJs": false,
    "sourceMap": true,
    "declaration": true,
    "stripInternal": true
