  "scripts": {
    "clean-lib": "shx rm -rf lib/vdp-* temp temp-esm",
    "build-lib-esm": "npm run clean-lib && tsc -m es6 && webpack --config webpack.prod.js && dts-bundle-generator --no-check --project ./tsconfig.json -o lib/vdp-lib.d.ts lib-main.ts",
    "build-lib": "npm run clean-lib && tsc && webpack --config webpack.prod.js && dts-bundle-generator --no-check --project ./tsconfig.json -o lib/vdp-lib.d.ts lib-main.ts",
    "build-app": "shx rm -rf temp temp-esm && tsc && webpack --config webpack.exe.js",
    "build-exe": "pkg -t node8-win-x86 -o packer tools/gfxConverter/executable.js",
    "app-dev-server": "webpack-dev-server --config webpack.dev.js",
    "pack-gfx": "node tools/gfxConverter/executable.js --convert-only",
    "pack-and-serve": "node tools/gfxConverter/executable.js"
  },
